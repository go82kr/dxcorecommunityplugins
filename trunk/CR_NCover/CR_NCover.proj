<Project DefaultTargets="build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003"> 
  <PropertyGroup>
    <TypeMockLocation>$(MSBuildStartupDirectory)/common/typemock</TypeMockLocation>
  </PropertyGroup>
  <Target Name="build">
    <MSBuild Projects="CR_NCover.sln" Properties="Configuration=Release;OutputPath=$(MSBuildStartupDirectory)/bin"/>
  </Target>
  <UsingTask TaskName="NUnit" AssemblyFile="$(teamcity_dotnet_nunitlauncher_msbuild_task)"/>
  <Import Project ="$(TypeMockLocation)\TypeMock.MSBuild.Tasks"/>
  <Target Name="test">
    <TypeMockStart/>
	<NUnit Assemblies="$(MSBuildStartupDirectory)/bin/CR_NCover.Tests.dll" Platform="x86"/>
	<TypeMockStop/>
  </Target>
</Project>